{
  "name": "netbrothers-gmbh/nbcsb-bundle",
  "description": "Creating skeleton structure for symfony bundle with one single command. Creates directory structure and necessary files, activates your bundle. See: https://netbrothers.de/easy-create-reusable-symfony-bundle",
  "type": "symfony-bundle",
  "keywords": ["symfony","bundle","helper","symfony command line"],
  "homepage": "https://github.com/netbrothers-gmbh/nbcsb-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Stefan Wessel",
      "email": "info@netbrothers.de",
      "homepage": "https://netbrothers.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/console": ">=7 <8",
    "symfony/serializer": ">=7 <8",
    "symfony/string": ">=7 <8",
    "symfony/yaml": ">=7 <8"
  },
  "autoload": {
    "psr-4": {
      "NetBrothers\\NbCsbBundle\\": "src/"
    }
  }
}
